ssh alfa sudo ip route change unreachable 10.0.0.56/30 table ntk_from_00:16:3E:EC:A3:E1
ssh alfa sudo ip route change unreachable 10.0.0.20/30 table ntk_from_00:16:3E:EC:A3:E1
